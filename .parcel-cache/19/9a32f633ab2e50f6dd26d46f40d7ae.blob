ddEventListener(\"mousemove\", e => {\n    cursor.x = e.clientX;\n    cursor.y = e.clientY;\n  });\n}\nlet speed = 0;\n\n// function animate() {\n//   c.fillStyle = \"rgba(255,255,255,0.2)\";\n//   c.fillRect(0, 0, innerWidth, innerHeight);\n\n//   requestAnimationFrame(animate);\n// }\n\nsetup();\n// animate();\n"],"names":["$75f2dfa7a25db45a5a75389f1a4b0862$var$canvas","document","querySelector","getContext","$75f2dfa7a25db45a5a75389f1a4b0862$var$fitCanvas","width","innerWidth","height","innerHeight","$75f2dfa7a25db45a5a75389f1a4b0862$var$cursor","x","y","window","addEventListener","e","clientX","clientY"],"version":3,"file":"index.HASH_REF_b4b14898711279480d1de6ad7fc7283b.js.map"}